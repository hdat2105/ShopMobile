
@model IEnumerable < ShopMobileConnection.NhaSanXuat>
<li class="dropdown">
    <a href="#" class="dropdown-toggle" data-toggle="dropdown">Điện Thoại</a>
    <div class="dropdown-menu">
        <div class="dropdown-inner">
            <ul class="list-unstyled">
                @foreach(var Item in  Model)
            {
                <li><a href="@Url.Action("Index","NhaSanXuat",new { id = Item.MaNhaSanXuat})">@Item.TenNhaSanXuat</a></li>
            }
            </ul>
        </div>
    </div>
</li>

